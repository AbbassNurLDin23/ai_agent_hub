{
  "name": "backend",
  "type": "commonjs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"No lint script configured\" && exit 0",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:push": "dotenv -e ../.env -- prisma db push",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:deploy": "dotenv -e ../.env -- prisma migrate deploy",
    "prisma:status": "dotenv -e ../.env -- prisma migrate status",
    "prisma:studio": "dotenv -e ../.env -- prisma studio"
  },
