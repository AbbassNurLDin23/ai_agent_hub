# Environment Variables Example
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ai_agent_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ai_agent_db
POSTGRES_PORT=5432

# Backend Configuration
PORT=4000
NODE_ENV=development
CLIENT_ORIGIN=http://localhost:5173

# LLM API Keys
OPENAI_API_KEY=sk-your-openai-api-key-here
GOOGLE_API_KEY=AIzaSy-your-google-api-key-here
GROQ_API_KEY=gsk_your-groq-api-key-here

# Frontend Configuration
VITE_API_URL=http://localhost:4000
VITE_PORT=8080

# Model Configuration (JSON array string)
VITE_MODELS_JSON=[{"key":"gsk_your-key","name":"LLaMA 3.3 70B","description":"Fast model","provider":"groq"}]
